      <div class="col-sm-2 col-xs-4 wow rollIn" data-wow-delay="0.4s"><img class="img img-responsive" src=<%=asset_path('twitter.png')%>/></div>

      <div class="col-sm-2 col-xs-4 wow rollIn" data-wow-delay="0.5s"><img class="img img-responsive" src=<%=asset_path('g_plus.png')%>/></div>

      <div class="col-sm-2 col-xs-4 wow rollIn" data-wow-delay="0.6s"><img class="img img-responsive" src=<%=asset_path('skype.png')%>/></div>

