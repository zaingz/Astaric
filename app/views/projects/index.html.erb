<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th>Tittle</th>
      <th>Catagory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.tittle %></td>
        <td><%= project.catagory %></td>
        <td><%= link_to 'Show',  project_with_title_path(project, :title => project.tittle) %></td>
        <td><%= link_to 'Edit', edit_project_path(project,:title => project.tittle) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
